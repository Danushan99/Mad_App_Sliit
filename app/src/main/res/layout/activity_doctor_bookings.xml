<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".doctor.doctorBookings"
    android:background="@drawable/newbg1">

    <androidx.cardview.widget.CardView
        android:layout_width="403dp"
        android:layout_height="149dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.214">



        <TextView
            android:id="@+id/textView5"
            android:layout_width="153dp"
            android:layout_height="62dp"
            android:layout_marginTop="50dp"
            android:gravity="center"
            android:hint="@string/docname1"
            android:textColorHint="#000000" />

        <Button
            android:id="@+id/edititemsBtn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="60dp"
            android:text="EDIT"
            app:cornerRadius="15dp" />

        <ImageView

            android:id="@+id/imageView3"
            android:layout_width="96dp"
            android:layout_height="76dp"
            android:layout_marginStart="150dp"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/doctormen" />
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="405dp"
        android:layout_height="132dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="150dp"
            android:layout_height="65dp"
            android:layout_marginTop="50dp"
            android:gravity="center"
            android:hint="@string/docname2"
            android:textColorHint="#000000" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="60dp"
            android:text="EDIT"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="127dp"
            android:layout_height="63dp"
            android:layout_marginStart="150dp"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/doctormen" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/allorders"
        android:layout_width="217dp"
        android:layout_height="98dp"
        android:layout_marginTop="16dp"
        android:hint="@string/bagtext"
        android:textAlignment="center"
        android:textColorHint="#000000"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>